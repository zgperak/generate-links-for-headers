{
    "manifest_version": 2,
    "author": "Tomislav Perkovic",
    "homepage_url": "https://github.com/zgperak/generate-links-for-headers",

    "name": "Generate Links on E-sfera",
    "description": "Automatically generates links for all sections in the DDS, to make it easier to share specific sections of the DDS.",
    "version": "1.0.0",

    "update_url": "http://clients2.google.com/service/update2/crx",

    "content_scripts": [
        {
            "css": [
                "generate_links.css"
            ],
            "js": [
                "generate_links.js"
            ],
            "matches": [
                "*://www.e-sfera.hr/*"
            ],
            "exclude_matches": [
            ]
        }
    ],

    "icons": {
        "16": "icons/e-sfera-generate-links-16.png",
        "32": "icons/e-sfera-generate-links-32.png",
        "48": "icons/e-sfera-generate-links-48.png",
        "64": "icons/e-sfera-generate-links-64.png",
        "128": "icons/e-sfera-generate-links-128.png"
    }
}
